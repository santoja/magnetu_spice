{
    "name": "santoja/magnetu_spice",
    "description": "Shared custom libraries for the Magnetu/Wazaper project",
    "type": "library",
    "require": {
        "php": ">=8.3"
    },
    "autoload": {
        "psr-4": {
           "Magnetu\\Spice\\": "src/"
        }

    },
    "autoload-dev": {
        "psr-4": {
            "Magnetu\\Spice\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "phpspec/prophecy-phpunit": "^2.2",
        "fakerphp/faker": "^1.23",
        "doctrine/doctrine-fixtures-bundle": "^3.6"
    }
}
